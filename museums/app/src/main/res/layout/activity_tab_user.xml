<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/white">
    <FrameLayout

        android:id="@+id/container_tab_user"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    </FrameLayout>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/user_btn_nav_menu"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="bottom"
        android:background="@color/lightBeige"
        app:itemIconSize="@dimen/end_icon_min_width"
        app:itemIconTint="@drawable/state_buttons"
        app:labelVisibilityMode="unlabeled"
        app:menu="@menu/menu_tab_user">

    </com.google.android.material.bottomnavigation.BottomNavigationView>

</LinearLayout>